{
    "name": "su-sws/jumpstart",
    "description": "Stanford Web Services Jumpstart",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Stanford Web Services",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "davidbarratt/custom-installer": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.1",
        "drush/drush": "~8.0",
        "drupal/console": "~1.0",
        "drupal/ctools": "~8.0",
        "drupal/features": "~8.0",
        "drupal/radix": "~8.3.0",
        "drupal/xmlsitemap": "~8.0",
        "drupal/panels": "~8.3",
        "drupal/page_manager": "~8.1",
        "drupal/panopoly": "~8.2",
        "drupal/layout_plugin": "~8.1",
        "drupal/field_group": "~8.1",
        "drupal/admin_toolbar": "~8.1",
        "drupal/linkicon": "~8.1",
        "drupal/metatag": "~8.1",
        "drupal/simple_sitemap": "~8.2",
        "drupal/video_embed_field": "~8.1",
        "drupal/embed": "~8.1",
        "drupal/media_entity": "~8.1",
        "drupal/media_entity_image": "~8.1",
        "drupal/entity": "~8.1",
        "drupal/entity_embed": "~8.1",
        "drupal/better_normalizers": "~8.1",
        "drupal/default_content": "@dev",
        "drupal/config_devel": "~8.1",
        "drupal/media_entity_browser": "@dev",
        "drupal/entity_browser": "@dev",
        "drupal/inline_entity_form": "@dev",
        "drupal/password_policy": "@dev"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "custom-installer": {
            "web/modules/stanford/{$name}/": [
                "type:stanford-module"
            ],
            "web/themes/stanford/{$name}/": [
                "type:stanford-theme"
            ],
            "web/modules/custom/{$name}/": [
                "type:custom-module"
            ],
            "web/themes/custom/{$name}/": [
                "type:custom-theme"
            ]
        }

    },
    "config": {
      "bin-dir": "bin/"
    }
}
