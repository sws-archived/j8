{
    "name": "su-sws/jumpstart",
    "description": "Stanford Web Services Jumpstart",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Stanford Web Services",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "davidbarratt/custom-installer": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "enyo/dropzone": "^v4.3.0",
        "drupal/core": "~8.1.0",
        "drush/drush": "~8.0",
        "drupal/console": "~1.0",
        "drupal/ctools": "@dev",
        "drupal/features": "^3.0",
        "drupal/radix": "^8.3.0",
        "drupal/panels": "@dev",
        "drupal/panelizer": "@dev",
        "drupal/page_manager": "dev-1.x",
        "drupal/panopoly": "@dev",
        "drupal/panopoly_widgets": "^8.2.0",
        "drupal/panopoly_theme": "^8.2.0",
        "drupal/panopoly_images": "^8.2.0",
        "drupal/panopoly_pages": "^2.0.0",
        "drupal/layout_plugin": "~8.1.0",
        "drupal/field_group": "dev-1.x",
        "drupal/group": "@dev",
        "drupal/admin_toolbar": "~8.1.0",
        "drupal/linkicon": "~8.1.0",
        "drupal/simple_sitemap": "~8.2.0",
        "drupal/video_embed_field": "~8.1.0",
        "drupal/embed": "~8.1.0",
        "drupal/entity_browser": "@dev",
        "drupal/media_entity": "~8.1.0",
        "drupal/media_entity_browser": "@dev",
        "drupal/media_entity_instagram": "^8.1.0",
        "drupal/media_entity_image": "^8.1.0",
        "drupal/media_entity_slideshow": "^8.1.0",
        "drupal/media_entity_twitter": "^8.1.0",
        "drupal/media_entity_document": "^8.1.0",
        "drupal/entity": "~8.1.0",
        "drupal/entity_embed": "~8.1.0",
        "drupal/better_normalizers": "~8.1.0",
        "drupal/default_content": "@dev",
        "drupal/config_devel": "~8.1.0",
        "drupal/config_update": "^8.1.0",
        "drupal/inline_entity_form": "@dev",
        "drupal/password_policy": "@dev",
        "drupal/asset_injector": "~8.1.0.0",
        "drupal/block_class": "8.*",
        "drupal/token": "^8.1.0",
        "drupal/editor_file": "8.1.1",
        "drupal/redirect": "~8.1.0",
        "drupal/xmlsitemap": "~8.1.0",
        "drupal/calendar": "8.*",
        "drupal/views_templates": "~8.1.0",
        "drupal/devel": "~8.1.0",
        "drupal/title": "~8.2.0",
        "drupal/crop": "^8.1.0",
        "drupal/breakpoint_js_settings": "^8.1.0",
        "drupal/dropzonejs": "dev-8.x-1.x#23e0d81de407c74c10333e65fb5d4fae09494e09",
        "drupal/entity_reference_revisions": "^8.1.0",
        "drupal/focal_point": "dev-8.x-1.x#e08d5c555f5daf61c35f639762637b119f6a5c8e",
        "drupal/google_analytics": "^8.2.0",
        "drupal/libraries": "dev-8.x-3.x#548f3c2c47596c5242e37b18000e7b2ec930f66a",
        "drupal/linkit": "^8.4.0",
        "drupal/metatag": "^8.1.0",
        "drupal/paragraphs": "^8.1.0",
        "drupal/pathauto": "@dev",
        "drupal/scheduler": "^8.1.0",
        "drupal/views_load_more": "dev-8.x-1.x#178d32195253207cc74097296e25079a23f0b2e5",
        "drupal/captcha": "dev-1.x",
        "drupal/address": "@dev",
        "drupal/dynamic_entity_reference": "@dev",
        "drupal/view_unpublished": "@dev",
        "drupal/search_api": "@dev",
        "drupal/scheduled_updates": "^8.1.0",
        "drupal/workbench_moderation": "^8.1.1",
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/coder": "8.*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "custom-installer": {
            "web/modules/stanford/{$name}/": [
                "type:stanford-module"
            ],
            "web/themes/stanford/{$name}/": [
                "type:stanford-theme"
            ],
            "web/modules/custom/{$name}/": [
                "type:custom-module"
            ],
            "web/themes/custom/{$name}/": [
                "type:custom-theme"
            ]
        },
        "patches": {
            "drupal/password_policy": {
                "patch two": "https://www.drupal.org/files/issues/2697777-php7-1.patch"
            },
            "drupal/core": {
                "Plugin Lazy loading can cause usort warning":
                "https://www.drupal.org/files/issues/Iterating-through-array--2699157.patch"
            },
            "drupal/ctools": {
                "2747739 - Mega patch":
                "https://www.drupal.org/files/issues/ctools--8.x-3.0-alpha26--1fe3649.patch"
            },
            "drupal/page_manager": {
                "https://www.drupal.org/node/2601004":
                "https://www.drupal.org/files/issues/page-manager-contextual-temp.patch"
            },
            "drupal/panelizer": {
                "2747739 - Mega Patch":
                "https://www.drupal.org/files/issues/panelizer--alpha2-1ddfa61--2747739-5.patch",
                "2701349 - Generate new UUIDs for displays when switching from default to field storage":
                "https://www.drupal.org/files/issues/2701349-2.patch",
                "2700597 - Explicitly set the Panels IPE URL root when saving in Panelizer":
                "https://www.drupal.org/files/issues/panelizer-ipe-url-root-handling.patch",
                "2664682 - Implement admin UI for editing Panelizer defaults":
                "https://www.drupal.org/files/issues/2664682-49.patch"
            },
            "drupal/panels": {
                "2747739 - Mega Patch":
                "https://www.drupal.org/files/issues/panels--beta4-4c4a3a7--2747739-17--binary.patch",
                "2667754 - Allow other modules to disable the IPE based on custom logic":
                "https://www.drupal.org/files/issues/2667754-3.patch",
                "Bandaid tempstore patch":
                "https://www.drupal.org/files/issues/bandaid.patch",
                "2697587 - Wizard API integration":
                "https://www.drupal.org/files/issues/2697587-48.patch"
            },
            "drush/drush": {
                "Adding composer.json support to make-convert command":
                "https://github.com/drush-ops/drush/commit/ce82b946d49b09cd33da5ca84feb24a6c35f8f8e.patch"            }
        }
    },
    "config": {
      "bin-dir": "bin/"
    }
}
